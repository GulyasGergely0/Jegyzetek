print("---------------4.---------------")

lista=["alma1","alma2","alma3","alma4","alma5"]

try:
    kerdes=int(input("Hanyadik index "))
    kerdes -= 1
except:
    print("Nem számot adtál meg")

try:
    print(lista[kerdes])
except:
    print("A listában nem létezik ez az index")



