
print("---------------1.---------------")

szam1=int(input("Első szám "))
szam2=int(input("Második szám "))

try:
    osztott=szam1 / szam2
    print(osztott)
except:
    print("Valami hiba történt az osztással")
    

print("---------------2.---------------")


try:
    szam=int(input("Szám "))
    ujszam= szam ** 2
    print(ujszam)
except:
    print("nem számot adtál meg!")
    
